{
    "extends": ["eslint:recommended", "prettier"],
    "rules": {
        "no-console": "error"
    },
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "env": {
        "jest": true
    },
    "globals": {
        "page": true,
        "browser": true,
        "context": true,
        "puppeteerConfig": true,
        "jestPuppeteer": true,
        "regeneratorRuntime": true
    }
}

{
  "name": "@daikin-oss/design-system-web-components",
  "version": "0.0.0",
  "description": "Design system using web components",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./dist/es/*": {
      "import": "./dist/es/*",
      "require": "./dist/cjs/*"
    }
  },
  "scripts": {
    "build": "tsc -b && vite build",
    "storybook": "storybook dev -p 6006",
    "storybook:react": "storybook dev -p 6007 -c .storybook/react",
    "build-storybook": "storybook build",
    "storybook:test": "storybook dev -p 6099 --ci",
    "storybook:test-react": "storybook dev -p 6099 --ci -c .storybook/react",
    "test:visual": "STORYBOOK_FW=web-components playwright test",
    "test:visual-react": "STORYBOOK_FW=react playwright test",
    "test": "npm run test:visual && npm run test:visual-react"
  },
  "dependencies": {
    "@daikin-oss/dds-tokens": "^0.0.0",
    "@netlify/classnames-template-literals": "^1.0.3",
    "lit": "^3.1.4"
  },
  "devDependencies": {
    "@daikin-oss/tailwind": "^0.0.1",
    "@fontsource/roboto": "^5.0.13",
    "@lit/react": "^1.0.5",
    "@playwright/test": "^1.45.0",
    "@storybook/addon-a11y": "^8.1.9",
    "@storybook/addon-essentials": "^8.1.9",
    "@storybook/addon-links": "^8.1.9",
    "@storybook/addon-mdx-gfm": "^8.1.9",
    "@storybook/blocks": "^8.1.9",
    "@storybook/manager-api": "^8.1.9",
    "@storybook/preview-api": "^8.1.9",
    "@storybook/react": "^8.1.9",
    "@storybook/react-vite": "^8.1.9",
    "@storybook/test": "^8.1.9",
    "@storybook/theming": "^8.1.9",
    "@storybook/web-components": "^8.1.9",
    "@storybook/web-components-vite": "^8.1.9",
    "@types/jest-image-snapshot": "^6.4.0",
    "@types/react": "^18.3.3",
    "autoprefixer": "^10.4.19",
    "class-variance-authority": "^0.7.0",
    "playwright": "^1.44.1",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.1.9",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-externalize-deps": "^0.8.0"
  }
}

{
  "name": "@daikin-oss/design-system-web-components",
  "version": "0.2.0",
  "description": "Design system using web components",
  "type": "module",
  "files": [
    "dist",
    "icons",
    "README.md",
    "LICENSE"
  ],
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/es/index.js",
  "types": "./dist/es/index.d.ts",
  "exports": {
    ".": {
      "development": {
        "import": "./dist/es-dev/index.js",
        "require": "./dist/cjs-dev/index.cjs"
      },
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/es/*.js"
    },
    "./dist/*.js": {
      "development": {
        "import": "./dist/es-dev/*.js",
        "require": "./dist/cjs-dev/*.cjs"
      },
      "import": "./dist/es/*.js",
      "require": "./dist/cjs/*.cjs",
      "default": "./dist/es/*.js"
    }
  },
  "sideEffects": true,
  "scripts": {
    "prepare": "husky",
    "build": "tsc -b && vite build && vite build --mode development",
    "storybook": "storybook dev -p 6006",
    "storybook:react": "storybook dev -p 6007 -c .storybook/react",
    "storybook:ci-wc": "tsx scripts/storybook-ci.ts web-components",
    "storybook:ci-react": "tsx scripts/storybook-ci.ts react",
    "storybook:build-wc": "storybook build -o storybook-static/web-components",
    "storybook:build-react": "storybook build -c .storybook/react -o storybook-static/react",
    "build-storybook": "npm run storybook:build-wc && npm run storybook:build-react",
    "storybook:preview-wc": "http-server -c-1 -d false --no-dotfiles -p 6106 storybook-static/web-components",
    "storybook:preview-react": "http-server -c-1 -d false --no-dotfiles -p 6107 -o storybook-static/react",
    "lint:code": "eslint && prettier -c .",
    "lint:cspell": "cspell --no-progress .",
    "lint:lit": "lit-analyzer --strict --rules.no-missing-import off src",
    "lint:ls": "ls-lint",
    "lint": "npm run lint:code && npm run lint:lit && npm run lint:cspell && npm run lint:ls",
    "lint-fix": "eslint --fix && prettier -w .",
    "test:unit": "vitest",
    "test:interaction-wc": "start-server-and-test storybook:ci-wc http://localhost:6099 \"npx test-storybook --url http://localhost:6099\"",
    "test:interaction-react": "start-server-and-test storybook:ci-react http://localhost:6099 \"npx test-storybook --url http://localhost:6099\"",
    "test:interaction": "npm run test:interaction-wc && npm run test:interaction-react",
    "test:visual-wc": "cross-env STORYBOOK_FW=web-components playwright test",
    "test:visual-react": "cross-env STORYBOOK_FW=react playwright test",
    "test:visual": "npm run test:visual-wc && npm run test:visual-react",
    "test:visual-update": "npm run test:visual-wc -- --update-snapshots"
  },
  "dependencies": {
    "@daikin-oss/dds-tokens": "^0.1.0",
    "@netlify/classnames-template-literals": "^1.0.3",
    "class-variance-authority": "^0.7.0",
    "lit": "^3.1.4"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.9.1",
    "@daikin-oss/tailwind": "^0.0.1",
    "@egoist/tailwindcss-icons": "^1.8.1",
    "@eslint/js": "^9.6.0",
    "@fontsource/roboto": "^5.0.13",
    "@lit/react": "^1.0.5",
    "@ls-lint/ls-lint": "^2.2.3",
    "@playwright/test": "^1.45.1",
    "@storybook/addon-a11y": "^8.2.1",
    "@storybook/addon-essentials": "^8.2.1",
    "@storybook/addon-interactions": "^8.2.1",
    "@storybook/addon-links": "^8.2.1",
    "@storybook/addon-mdx-gfm": "^8.2.1",
    "@storybook/blocks": "^8.2.1",
    "@storybook/builder-vite": "^8.2.1",
    "@storybook/manager-api": "^8.2.1",
    "@storybook/preview-api": "^8.2.1",
    "@storybook/react": "^8.2.1",
    "@storybook/react-vite": "^8.2.1",
    "@storybook/test": "^8.2.1",
    "@storybook/test-runner": "^0.19.0",
    "@storybook/theming": "^8.2.1",
    "@storybook/web-components": "^8.2.1",
    "@storybook/web-components-vite": "^8.2.1",
    "@types/eslint__js": "^8.42.3",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/jest-image-snapshot": "^6.4.0",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "cspell": "^8.10.4",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-lit": "^1.14.0",
    "eslint-plugin-lit-a11y": "^4.1.3",
    "eslint-plugin-wc": "^2.1.0",
    "fast-glob": "^3.3.2",
    "http-server": "^14.1.1",
    "husky": "^9.0.11",
    "lightningcss": "^1.25.1",
    "lit-analyzer": "^2.0.3",
    "picocolors": "^1.0.1",
    "playwright": "^1.45.1",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "prettier-plugin-organize-imports": "^3.2.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "shadow-dom-testing-library": "^1.11.2",
    "start-server-and-test": "^2.0.4",
    "storybook": "^8.2.1",
    "tailwindcss": "^3.4.4",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.0-alpha.41",
    "vite": "^5.3.3",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-externalize-deps": "^0.8.0",
    "vitest": "^2.0.4"
  }
}

{
  "files.associations": {
    "cspell-dictionary.txt": "ignore"
  },
  "tailwindCSS.experimental.classRegex": [
    // cva
    ["\\[((?:[^[]|\\[[^\\]]*\\])+)\\]", "\"((?:\\\\.|[^\"])+)\""],
    // class / className
    ["class(?:Name)?=\"((?:\\\\.|[^\"])+)\""],
    // /* tw */
    ["/\\*\\s*tw\\s*\\*/\\s*\"((?:\\\\.|[^\"])+)\""]
  ]
}
